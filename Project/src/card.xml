<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="card">

	<!-- 혜택에 해당된 카드를 불러옴-->
	<select id="card_list"  parameterType="java.util.Map"  resultType="card">
	    <![CDATA[
		select * from card where card_ad = #{card_ad}
		]]>
	</select>
	
	<select id="card_list_all"  parameterType="java.util.Map"  resultType="card">
	    <![CDATA[
		select * from card
		]]>
	</select>

	
</mapper>




















